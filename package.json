  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint src --ext ts,tsx --no-eslintrc --config .eslintrc-default.json",
    "lint:fix": "npx eslint src --ext ts,tsx --no-eslintrc --config .eslintrc-default.json --fix",
