    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-styled-components": "^2.1.4",
    "dompurify": "^3.0.5",
    "highlight.js": "^11.8.0",
    "localforage": "^1.10.0",
    "marked": "^5.1.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.0.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": "./.eslintrc.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
